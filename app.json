{
  "expo": {
    "name": "Mobile 193",
    "slug": "mobile193f",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "assets/images/splash.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ff2825"
    },
    "extra": {
      "eas": {
        "projectId": "fe5225dc-ce32-4cba-a006-735a98b91e88"
      }
    },
    "android": {
      "package": "com.mottaeduardo.mobile193f"
    }
  },
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "assets/images/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "permissions": [
      "ACCESS_FINE_LOCATION",
      "ACCESS_COARSE_LOCATION",
      "android.permission.ACCESS_COARSE_LOCATION",
      "android.permission.ACCESS_FINE_LOCATION"
    ],
    "package": "com.mottaeduardo.mobile193f"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "assets/images/favicon.png"
  },
  "plugins": [
    [
      "expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "O app precisa acessar sua localização para emergências."
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  },
  "splash": {
    "image": "./assets/images/splash.png",
    "backgroundColor": "#ff2825"
  },
  "extra": {
    "router": {
      "origin": false
    },
    "eas": {
      "projectId": "fe5225dc-ce32-4cba-a006-735a98b91e88"
    }
  }
}
